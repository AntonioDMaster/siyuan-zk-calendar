  "scripts": {
    "make-link": "node ./scripts/make_dev_link.js",
    "dev": "vite build --watch --mode dev",
    "build": "pnpm run type-check && pnpm run build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "commit": "git cz",
    "amend": "git commit --amend --no-edit",
    "release": "standard-version && pnpm run push-tag",
    "push-tag": "git push --follow-tags origin main",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.54.1",
    "vue": "^3.4.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.10.8",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.0",
    "cz-git": "^1.8.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "fast-glob": "^3.3.2",
    "less": "^4.2.0",
    "minimist": "^1.2.8",
    "prettier": "^3.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "siyuan": "^0.9.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.11",
    "vite-plugin-static-copy": "^1.0.0",
    "vite-plugin-zip-pack": "^1.0.7",
    "vue-tsc": "^1.8.27"
  }
}
