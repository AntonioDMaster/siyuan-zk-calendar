    "standard-version": "^9.5.0",
    "typescript": "^5.1.3",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-static-copy": "^0.16.0",
    "vite-plugin-zip-pack": "^1.0.5",
    "vue-tsc": "^1.6.5"
  }
